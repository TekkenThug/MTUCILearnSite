{
  "extends": [
    "plugin:import/recommended",
    "plugin:vue/recommended",
    "airbnb-base"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@babel/eslint-parser",
    "requireConfigFile": false
  },
  "rules": {
    "vue/multi-word-component-names": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-shadow": "warn",
    "no-plusplus": "off"
  }
}
